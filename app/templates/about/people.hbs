
<div class="panel panel-default">
  <div class="panel-heading"><h5>People Page</h5></div>
  <div class="panel-body">
    <table class="table table-striped table-hover table-condensed">
      <caption>Faker table caption</caption>
      <thead>
        <tr>
          <th>Index</th>
          <th>Item Id</th>
          <th>Contact Name</th>
          <th>Age</th>
          <th>Zip</th>
        </tr>
      </thead>
      <tbody>
        {{#each model as |item index|}}
          <tr class="contact">
            <td>{{index}}</td>
            <td>{{item.id}}</td>
            <td class="contactName">{{item.firstName}} {{item.lastName}}</td>
            <td>{{item.age}}</td>
            <td>{{item.zip}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>
